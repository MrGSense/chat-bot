{
  "name": "chat-bot",
  "version": "1.0.0",
  "description": "Chat bot made using DiaglogFlow in the MERN Stack",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "nodemon server.js",
    "frontend": " npm run start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install -D --prefix client && npm run build --prefix client"
  },
  "author": "Kyle Fetterhoff",
  "license": "ISC",
  "dependencies": {
    "actions-on-google": "^2.12.0",
    "dialogflow": "^1.2.0",
    "dialogflow-fulfillment": "^0.6.1",
    "express": "^4.17.1",
    "mongoose": "^5.9.2"
  },
  "engines": {
    "node": "13.1.0",
    "npm": "6.12.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
